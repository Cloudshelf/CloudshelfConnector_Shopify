{
  "name": "cloudshelfconnector_shopify",
  "version": "0.0.1",
  "description": "Shopify connector for Cloudshelf",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudshelf/cloudshelfconnector_shopify.git"
  },
  "scripts": {
    "dev": "nodemon src/main.ts",
    "codegen:cloudshelf": "graphql-codegen --config src/graphql/cloudshelf/codegen.ts",
    "codegen:shopifyAdmin": "graphql-codegen --config src/graphql/shopifyAdmin/codegen.ts",
    "codegen:shopifyStorefront": "graphql-codegen --config src/graphql/shopifyStorefront/codegen.ts",
    "codegen:database": "kysely-codegen",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "build": "tsc"
  },
  "author": "Julian Forrester (TreeOfLearning)",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/cloudshelf/cloudshelfconnector_shopify/issues"
  },
  "homepage": "https://github.com/cloudshelf/cloudshelfconnector_shopify#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/oauth": "^0.9.1",
    "@types/request-promise-native": "^1.0.18",
    "@types/uuid": "^9.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@mikro-orm/cli": "^5.7.14",
    "@mikro-orm/core": "^5.7.14",
    "@mikro-orm/migrations": "^5.7.14",
    "@mikro-orm/postgresql": "^5.7.14",
    "@shopify/shopify-api": "^7.6.0",
    "@shopify/shopify-app-express": "^2.2.2",
    "@shopify/shopify-app-session-storage": "^1.1.9",
    "@shopify/shopify-app-session-storage-memory": "^1.0.11",
    "@shopify/shopify-app-session-storage-postgresql": "^1.1.10",
    "@shopify/shopify-app-session-storage-redis": "^1.1.9",
    "@slack/web-api": "^6.9.0",
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "bullmq": "^4.7.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "http-proxy-middleware": "^3.0.0-beta.1",
    "multer": "^1.4.5-lts.1",
    "oauth": "^0.10.0",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "routing-controllers": "^0.10.4",
    "uuid": "^9.0.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
